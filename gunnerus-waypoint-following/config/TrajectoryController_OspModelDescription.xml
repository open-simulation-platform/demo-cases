<?xml version="1.0" encoding="utf-8" ?>
<OspModelDescription xmlns="https://open-simulation-platform.com/OspModelDescription/1.0.0" version="1.0">
    <VariableGroups>
        
        <Generic name="position_3dof">
            <Variable ref="northPosition"/>
            <Variable ref="eastPosition"/>
            <Variable ref="headingAngle"/>
        </Generic>

        <Generic name="velocity_2dof">
            <Variable ref="surgeVelocity"/>
            <Variable ref="swayVelocity"/>
        </Generic>

        <Generic name="target_wp">
            <Variable ref="targetWP.north"/>
            <Variable ref="targetWP.east"/>
            <Variable ref="targetWP.speed"/>
        </Generic>

        <Generic name="previous_wp">
            <Variable ref="prevWP.north"/>
            <Variable ref="prevWP.east"/>
            <Variable ref="prevWP.speed"/>
        </Generic>

        <Generic name="force_command">
            <Variable ref="forceCommand"/>
        </Generic>
        
        <Generic name="rudder_command">
            <Variable ref="rudderCommand"/>
        </Generic>
    </VariableGroups>
</OspModelDescription>